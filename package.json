{
  "name": "budget-client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "config-hooks": "git config core.hooksPath ./git-hooks",
    "ng": "ng",
    "start": "node server.js",
    "heroku-postbuild": "npm run build:in-memory",
    "start:node": "npm run start:ci --prefix ./node",
    "build": "ng build --configuration production",
    "build:in-memory": "ng build -c production,in-memory",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock-server": "json-server integration/db.json --routes integration/rewrites.json",
    "test:integration": "ng test --testMatch **/integration/*.(it).ts",
    "test:pact": "ng test --testMatch **/contract/*.(pact).ts",
    "cypress:open": "cypress open",
    "eslint": "eslint src/**/*.{js,ts}",
    "eslint:fix": "eslint --fix src/**/*.{js,ts}",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "lint-staged": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^13.2.4",
    "@angular/animations": "^13.2.3",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^13.2.3",
    "@angular/compiler": "^13.2.3",
    "@angular/core": "^13.2.3",
    "@angular/forms": "^13.2.3",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^13.2.3",
    "@angular/platform-browser-dynamic": "^13.2.3",
    "@angular/router": "^13.2.3",
    "@briebug/jest-schematic": "^3.1.0",
    "angular-in-memory-web-api": "^0.10.0",
    "core-js": "^3.19.0",
    "express": "^4.17.1",
    "ng-qrcode": "^3.1.0",
    "path": "^0.12.7",
    "rxjs": "^6.6.7",
    "webpack-bundle-analyzer": "^3.9.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "latest",
    "@angular-devkit/build-angular": "^13.2.4",
    "@angular/cli": "^13.2.4",
    "@angular/compiler-cli": "^13.2.3",
    "@angular/language-service": "^13.2.3",
    "@pact-foundation/pact": "^9.16.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "latest",
    "@types/json-server": "^0.14.4",
    "@types/node": "^13.13.52",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "codelyzer": "^6.0.0",
    "cypress": "^8.7.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "latest",
    "json-server": "^0.17.0",
    "karma-cli": "~2.0.0",
    "lint-staged": "^12.0.3",
    "msw": "^0.35.0",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-node": "~8.6.2",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}